   <string notr="true">#Dashboard {
/* background: rgb(140, 225, 255) */
background: rgb(60, 139, 212)
}

#centralWidget {
background: rgb(60, 139, 212)
}

#mediaItemFrame {
background-color: rgb(217, 217, 217)
}

QPushButton {
background-color: rgb(17, 224, 255);
border-style: solid;
border-width: 1.5px;
border-radius: 15px;
border-color: black;
font-family: tahoma;
font: bold, 20px;
color: black;
padding: 0px;
text-align: center;
}

QPushButton:hover {
background-color: rgb(235, 247, 255)
}

QPlainTextEdit {
background-color: white;
border-color: rgb(149, 255, 247);
border-radius: 5px;
font-family: tahoma;
}

QLineEdit {
background-color: white;
border-color: rgb(149, 255, 247);
border-radius: 5px;
font-family: tahoma;
font-size: 12px;
}

QLabel {
font-family: verdana;
font: bold;
font-size: 14px;
}

QCheckBox {
font-family: tahoma;
font-size: 13px;
}

QListView {
background-color: rgb(217, 217, 217)
}

QListWidget {
background-color: rgb(217, 217, 217)
}

QTreeWidget {
background-color: rgb(217, 217, 217)
}

QTextEdit{
background-color: rgb(217, 217, 217)
}

#showName {
font-family: tahoma;
font-size: 20px;
font: bold
}

#seasonName {
font-family: tahoma;
font-size: 14px;
font: normal
}

#episodeName {
font-family: tahoma;
font-size: 14px;
font: normal
}

#episodeSummaryLabel {
font: normal;
font-size: 10px;
}

#episodeSummary {
font-family: tahoma;
font-size: 12px;
font: normal;
background: white;
border-style: solid;
border-width: 1.5px;
border-color: black;
}

#commentBoxLabel, #reviewCommentBoxLabel {
font-size: 10px;
font: normal
}

#ratingLabel {
font: normal;
font-size: 10px;
}

#overallRatingLabel {
font: normal;
}

#episodeRatingNum {
background-color: white;
font-size: 20px;
color: blue;
border-style: solid;
border-width: 1.5px;
border-radius: 5px;
border-color: black;
}

#ratingNumber {
background-color: white;
font-size: 14px;
font: bold;
}

QTableWidget {
background:  rgb(231, 225, 255);
gridline-color: gray;
border-radius: 8px;
}

#talkTab {
background-color:  rgb(60, 139, 212);
border-top: 2px solid rgb(28, 70, 206);
}

#reviewTab {
background-color:  rgb(60, 139, 212);
border-top: 2px solid rgb(28, 70, 206);
}

#commentBox {
border-radius: 8px;
}

#reviewCommentBox {
border-radius: 8px;
}

QTabBar::tab {
background: rgb(17, 224, 255);
border: 1.5px solid black;
border-top-left-radius: 8px;
border-top-right-radius: 8px;
min-width: 8ex;
padding: 2px;
font: bold;
font-size: 10px;
}

QTabBar::tab:hover {
    background: rgb(235, 247, 255)
}

QTabBar::tab:selected {
   background: rgb(60, 139, 212);
    border-color: black;
    border-bottom-color: black;
}

QTabBar::tab:!selected {
    margin-top: 2px;
}</string>
